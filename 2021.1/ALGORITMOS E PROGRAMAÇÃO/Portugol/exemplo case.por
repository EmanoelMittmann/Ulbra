programa {
	funcao inicio() {
		inteiro valor
		escreva("Digite qualquer valor inteiro:\n")
		leia(valor)
		
		escolha(valor){
		    caso 1:{
		            escreva("Você digitou 1\n")
		        pare
		    }
		    caso 2:{
		            escreva("Voce digitou 2\n")
		        pare
		    }
		    caso 3:{
		            escreva("Você digitou 3\n")
		        pare
		    }
		    caso contrario:{
		        escreva("Você digitou qualquer coisa")
		}
	}
	
	}
	
}
