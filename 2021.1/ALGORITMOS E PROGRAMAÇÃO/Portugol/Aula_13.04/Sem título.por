programa {
	funcao inicio() {
		inteiro i
		real a, b, c
		escreva("Digite um valor para A")
		leia(a)
		escreva("Digite um valor para B")
		leia(b)
		escreva("Digite um valor para C")
		leia(c)
		escreva("Digite um valor para I")
		leia(i)
		
		se (i==1){
		    escreva("Usuario digitou 1")
		}senao se (i==2){
		    escreva("Usuário digitou 2")
		}senao se (i==3){
		    escreva("Usuario digitou 3")
		}senao{
		    escreva("ESTRADA INVALIDA, ENTRADAS VALIDAS 1,2,3")
		}
		 
	}
}
