{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "prettier"

    ],
    "plugins":[
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules":{
        "prettier/prettier":[
            "error",
            {
                "endOfLine":"auto"
            }
        ],
        "react/react-in-jsx-scope": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-one-expression-per-line": 0,
        "quotes": 0,
        "no-console": 2
      }
}
